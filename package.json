{
  "name": "todo-cli-app",
  "version": "0.0.2",
  "description": "A Command Line Interface (CLI) TO-DO application built with TypeScript",
  "main": "./src/user/App.ts",
  "scripts": {
    "test": "set NODE_ENV=test && jest",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --fix --ext .ts ./src",
    "dev": "set NODE_ENV=dev&& ts-node-dev --inspect --respawn --pretty --transpile-only src/App.ts",
    "build": "rimraf dist && tsc",
    "start": "set NODE_ENV=prod&& node dist/src/App.js"
  },
  "author": "@metawiser & @xpan1c",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/colors": "^1.2.1",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^27.5.0",
    "@types/mongoose": "^5.11.97",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.16.3",
    "@types/uuid": "^9.0.1",
    "@types/validator": "^13.7.15",
    "eslint": "^8.36.0",
    "eslint-config-codely": "^2.1.3",
    "eslint-plugin-hexagonal-architecture": "^1.0.3",
    "mysql": "^2.18.1",
    "rimraf": "^4.4.1",
    "ts-jest": "^28.0.0",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "colors": "^1.4.0",
    "dotenv": "^16.0.3",
    "inquirer": "^8.2.5",
    "mongodb": "^5.2.0",
    "mongoose": "^7.1.0",
    "mysql2": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5",
    "uuid": "^9.0.0"
  }
}
